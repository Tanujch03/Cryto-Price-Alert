
generator client {
  provider = "prisma-client-js"
 
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model PriceAlert{
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  coinId           String
  targetPrice      Float
  email            String
  currentPrice     Float
  status           String
  createdAt        DateTime @default(now())
  time             DateTime @default(now())

}